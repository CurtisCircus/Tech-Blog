<!DOCTYPE html>
<html lang="en">

<!-- Include your layout and other HTML structure -->

<body>

  <h1>Homepage</h1>

  <!-- Display existing blog posts -->
  {{#each posts}}
    <div>
      <h2>{{title}}</h2>
      <p>{{content}}</p>
      <!-- Add other post details here -->
